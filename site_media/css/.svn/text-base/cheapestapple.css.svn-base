html, body{
      background-color: rgb(230, 230, 230);
      text-align: center;
      font-smooth: always;
}

#main_page{
	margin-left: auto;
	margin-right: auto;
	margin: auto;
	padding-bottom: 20px;
	position: relative;
	width: 960px;
	/*height: 150px;*/
	height: auto;
	background-color: rgb(255, 255, 255);
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}
#product_box_container{
	background-color: rgb(60, 60, 60);
	margin: auto;
	position: relative;
	width: 700px;
	height: 292px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}
.price_box{
	cursor: hand;
	cursor: pointer;
	background-color: rgb(60, 60, 60);
	height: 50px;
	width: 700px;
	position: relative;
	margin: auto;
	margin-top: 10px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}
.price_pic{
	height: 50px;
	width: 140px;
	float: left;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
		
}
.price_message{
	height: 36px;
	width: 500px;
	float: right;
	margin-top: 7px;
	margin-right: 30px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}
.product_box{
	cursor: hand;
	cursor: pointer;
	background-color: rgb(60, 60, 60);
	margin: auto;
	margin-top: 30px;
	position: relative;
	width: 700px;	
	height: 140px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}
.product_box2{
	background-color: rgb(60, 60, 60);
	margin: auto;
	margin-top: 30px;
	position: relative;
	width: 700px;	
	height: 140px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}
.price_link{
	color: rgb(255, 255, 255);
	margin: auto;
	margin-top: 4px;
	font-family: "Arial";
	font-size: 20px;
}
.product_pic{
	height: 112px;
	width: 140px;
	float: left;
	margin-left: 20px;
	margin-top: 14px;
	position: relative;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}
.product_title{
	float: right;
	height: 35px;
	width: 450px;
	position: relative;
	margin-left: 20px;
	margin-right: 20px;
	margin-top: 15px;
}
.product_line{
	float: right;
	height: 20px;
	width: 450px;
	position: relative;
	margin-left: 20px;
	margin-right: 20px;
	margin-top: 15px;
}
#break_box{
	background-color: rgb(60, 60, 60);
	position: relative;
	height: 5px;
	width: 100%;
	margin-bottom: 20px;
}
#top_message_box {
	background-color: rgb(60, 60, 60);
	width: 580px;
	height: 60px;
	margin: auto;	
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}
.white_break_box{
	margin: auto;
	height: 5px;
	width: 80%;
	position: relative;
	background-color: rgb(255, 255, 255);
	-moz-border-radius: 1px;
	-webkit-border-radius: 5px;
}
.box {
	cursor: hand;
	cursor: pointer;  
	background-color: rgb(60, 60, 60);	  
	margin:30px;
	float: left;
	position: relative;
	width: 250px;
	height: 250px;
	padding: 5px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;	
}
h1.title {
	font-family: "Arial";
	color: rgb(60, 60, 60);
	font-size: 30px;
	text-align: left;
	margin-top: 3px;
	margin-bottom: 3px;
	margin-left: 10px;
	margin-right: 10px;
}
h1.top_box {
	/*text-decoration: underline;*/
	color: white;
	font-family: "Arial";
	margin-bottom: 0px;
}
.top_link{
	text-decoration: none;
	color: white;
	font-family: "Arial";
	margin-bottom: 0px;
}
h1.top_message {
	/*text-decoration: underline;*/
	position: absolute;
	color: white;
	margin: 10px;
	font-family: "Arial";
}
h1.prod_title{
	/*text-decoration: underline;*/
	color: white;
	margin: auto;
	font-family: "Arial";
	font-size: 30px;
}
.prod_link {
	color: white;
	margin: auto;
	font-family: "Arial";
	font-size: 30px;
}
h1.prod_desc {
	/*text-decoration: underline;*/
	color: rgb(200, 200, 200);
	margin: auto;
	font-family: "Arial";
	font-size: 15px;
}
h1.price {
	/*text-decoration: underline;*/
	color: rgb(255, 255, 255);
	margin: auto;
	margin-top: 4px;
	font-family: "Arial";
	font-size: 20px;
}
